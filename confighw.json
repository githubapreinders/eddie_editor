{
    "elements": [
        {
            "type": "element",
            "name": "module",
            "elements": [
                {
                    "type": "element",
                    "name": "adapter",
                    "attributes": {
                        "name": "HelloWorld",
                        "active": "${HelloWorld.active}"
                    },
                    "elements": [
                        {
                            "type": "element",
                            "name": "receiver",
                            "attributes": {
                                "name": "HelloWorld",
                                "className": "nl.nn.adapterframework.receivers.GenericReceiver"
                            },
                            "elements": [
                                {
                                    "type": "element",
                                    "name": "listener",
                                    "attributes": {
                                        "name": "HelloWorld",
                                        "className": "nl.nn.adapterframework.receivers.JavaListener"
                                    }
                                }
                            ]
                        },
                        {
                            "type": "element",
                            "name": "pipeline",
                            "attributes": {
                                "firstPipe": "HelloWorld"
                            },
                            "elements": [
                                {
                                    "type": "element",
                                    "name": "exits",
                                    "elements": [
                                        {
                                            "type": "element",
                                            "name": "exit",
                                            "attributes": {
                                                "path": "EXIT",
                                                "state": "success"
                                            }
                                        }
                                    ]
                                },
                                {
                                    "type": "element",
                                    "name": "pipe",
                                    "attributes": {
                                        "name": "HelloWorld",
                                        "className": "nl.nn.adapterframework.pipes.FixedResult",
                                        "returnString": "Hello Precious World"
                                    },
                                    "elements": [
                                        {
                                            "type": "element",
                                            "name": "forward",
                                            "attributes": {
                                                "name": "success",
                                                "path": "EXIT"
                                            }
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "element",
                    "name": "scheduler",
                    "elements": [
                        {
                            "type": "element",
                            "name": "job",
                            "attributes": {
                                "name": "HelloWorld",
                                "adapterName": "HelloWorld",
                                "receiverName": "HelloWorld",
                                "interval": "0",
                                "function": "sendMessage",
                                "active": "${HelloWorld.active}"
                            }
                        }
                    ]
                }
            ]
        }
    ]
}